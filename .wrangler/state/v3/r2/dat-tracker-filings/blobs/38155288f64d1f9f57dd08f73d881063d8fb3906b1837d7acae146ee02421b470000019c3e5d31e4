    <td style="border-bottom: Black 1pt solid; font-weight: bold; text-align: left">$</td><td id="xdx_981_eus-gaap--Revenues_c20250101__20250331__us-gaap--FairValueByAssetClassAxis__custom--BitcoinMember_fKg_____zvPDOHsQ7Wha" title="Revenues" style="border-bottom: Black 1pt solid; font-weight: bold; text-align: right"><ix:nonFraction name="us-gaap:Revenues" contextRef="From2025-01-012025-03-31_custom_BitcoinMember" id="Fact000623" format="ixt:numdotdecimal" decimals="0" unitRef="USD">339,291</ix:nonFraction></td><td style="padding-bottom: 1pt; font-weight: bold; text-align: left">&#160;</td><td style="padding-bottom: 1pt">&#160;</td>
