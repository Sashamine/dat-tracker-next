    <td style="padding-left: 10pt; width: 28%; text-align: left">Crypto Assets</td><td style="width: 2%">&#160;</td>
    <td style="width: 1%; text-align: left">$</td><td id="xdx_98D_ecustom--CryptoAssets_iI_pp0p0_c20231231_z4PR351Gg7J4" title="Crypto Assets" style="width: 14%; text-align: right"><ix:nonFraction name="BTCS:CryptoAssets" contextRef="AsOf2023-12-31" id="Fact001178" format="ixt:numdotdecimal" decimals="0" scale="0" unitRef="USD">25,202,929</ix:nonFraction></td><td style="width: 1%; text-align: left">&#160;</td><td style="width: 2%">&#160;</td>
