    <td style="width: 1%; text-align: left">$</td><td style="width: 9%; text-align: right"><ix:nonFraction contextRef="c16" decimals="0" format="ixt:num-dot-decimal" name="us-gaap:NetIncomeLossAvailableToCommonStockholdersBasic" scale="0" unitRef="usd" id="ixv-10422">3,586,446</ix:nonFraction></td><td style="width: 1%; text-align: left">&#160;</td><td style="width: 1%">&#160;</td>
    <td style="width: 1%; text-align: left">$</td><td style="width: 9%; text-align: right"><ix:nonFraction contextRef="c18" decimals="0" format="ixt:num-dot-decimal" name="us-gaap:NetIncomeLossAvailableToCommonStockholdersBasic" scale="0" unitRef="usd" id="ixv-10423">857,188</ix:nonFraction></td><td style="width: 1%; text-align: left">&#160;</td><td style="width: 1%">&#160;</td>
