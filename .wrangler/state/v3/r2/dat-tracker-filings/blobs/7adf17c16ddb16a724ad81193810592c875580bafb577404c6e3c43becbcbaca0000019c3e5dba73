    <td style="text-align: left">Total fair value method holdings</td><td>&#160;</td>
    <td style="text-align: left">$</td><td id="xdx_98D_eus-gaap--EquitySecuritiesFvNiCost_iI_pn3n3_c20231231_zpCsmzfihQBg" title="Cost Basis" style="text-align: right"><ix:nonFraction name="us-gaap:EquitySecuritiesFvNiCost" contextRef="AsOf2023-12-31" id="Fact001532" format="ixt:numdotdecimal" decimals="-3" scale="3" unitRef="USD">8,679</ix:nonFraction></td><td style="text-align: left">&#160;</td><td>&#160;</td>
