    <td style="border-bottom: Black 1.5pt solid; width: 1%; text-align: left">$</td><td id="xdx_988_eus-gaap--EquitySecuritiesFvNiUnrealizedGain_pn3n3_c20230101__20231231__us-gaap--FinancialInstrumentAxis__custom--GreenFirstCommonStockMember_zz3dMQfL9Gbk" title="Gross Unrealized Gains" style="border-bottom: Black 1.5pt solid; width: 11%; text-align: right"><ix:nonFraction name="us-gaap:EquitySecuritiesFvNiUnrealizedGain" contextRef="From2023-01-012023-12-31_custom_GreenFirstCommonStockMember" id="Fact001526" format="ixt:numdotdecimal" decimals="-3" scale="3" unitRef="USD">1,873</ix:nonFraction></td><td style="width: 1%; padding-bottom: 1.5pt; text-align: left">&#160;</td><td style="width: 2%; padding-bottom: 1.5pt">&#160;</td>
    <td style="border-bottom: Black 1.5pt solid; width: 1%; text-align: left">$</td><td id="xdx_988_eus-gaap--EquitySecuritiesFvNiUnrealizedLoss_pn3n3_c20230101__20231231__us-gaap--FinancialInstrumentAxis__custom--GreenFirstCommonStockMember_zhKtmrO870Ya" title="Gross Unrealized Losses" style="border-bottom: Black 1.5pt solid; width: 11%; text-align: right"><span style="-sec-ix-hidden: xdx2ixbrl1528">-</span></td><td style="width: 1%; padding-bottom: 1.5pt; text-align: left">&#160;</td><td style="width: 2%; padding-bottom: 1.5pt">&#160;</td>
